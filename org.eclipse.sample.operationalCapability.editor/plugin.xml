<plugin>

   <extension point="org.eclipse.ui.newWizards">
      <!-- @generated operationalCapability -->
      <category
            id="org.eclipse.emf.ecore.Wizard.category.ID"
            name="%_UI_Wizard_category"/>
      <wizard
            id="org.eclipse.sample.operationalCapability.presentation.OperationalCapabilityModelWizardID"
            name="%_UI_OperationalCapabilityModelWizard_label"
            class="org.eclipse.sample.operationalCapability.presentation.OperationalCapabilityModelWizard"
            category="org.eclipse.emf.ecore.Wizard.category.ID"
            icon="icons/full/obj16/OperationalCapabilityModelFile.gif">
         <description>%_UI_OperationalCapabilityModelWizard_description</description>
         <selection class="org.eclipse.core.resources.IResource"/>
      </wizard>
   </extension>

   <extension point="org.eclipse.ui.editors">
      <!-- @generated operationalCapability -->
      <editor
            id="org.eclipse.sample.operationalCapability.presentation.OperationalCapabilityEditorID"
            name="%_UI_OperationalCapabilityEditor_label"
            icon="icons/full/obj16/OperationalCapabilityModelFile.gif"
            extensions="operationalcapability"
            class="org.eclipse.sample.operationalCapability.presentation.OperationalCapabilityEditor"
            contributorClass="org.eclipse.sample.operationalCapability.presentation.OperationalCapabilityActionBarContributor">
      </editor>
   </extension>

</plugin>
